
<% flash.each do |type, msg| %>
    <div class="flash-py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block<%= notice %>">
      <%= msg %>
    </div>
    <div class="flash-py-2 px-3 bg-red-50 mb-5 text-red-500 font-medium rounded-lg inline-block<%= alert %>">
    <%= msg %>
  </div>
  <% end %>